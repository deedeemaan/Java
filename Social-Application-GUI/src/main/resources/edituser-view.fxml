<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="229.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.map_toysocialnetwork_gui.controller.EditUserController">
    <children>
      <VBox prefHeight="229.0" prefWidth="431.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                      <Label prefHeight="43.0" prefWidth="65.0" text="First name:" />
                      <TextField fx:id="textFieldFN" prefHeight="46.0" prefWidth="220.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                      <Label prefHeight="41.0" prefWidth="64.0" text="Last name:" />
                      <TextField fx:id="textFieldLN" prefHeight="46.0" prefWidth="220.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="45.0" prefWidth="65.0" text="Password:" />
                  <TextField fx:id="textFieldPW" prefHeight="46.0" prefWidth="220.0" />
               </children>
            </HBox>
            <ButtonBar prefHeight="40.0" prefWidth="223.0">
              <buttons>
                  <Button mnemonicParsing="false" onAction="#handleSave" prefHeight="25.0" prefWidth="93.0" text="Save" />
                  <Button mnemonicParsing="false" onAction="#handleUpdate" text="Update" />
                  <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
    </children>
</AnchorPane>
